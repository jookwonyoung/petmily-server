{{>layout/header}}

<div class="col-md-12">


<table class="table table-horizontal table-bordered">
    <thead class="thead-strong">
    <tr>
        <th>산책번호</th>
        <th>년도</th>
        <th>월</th>
        <th>일</th>
        <th>시간</th>
        <th>이동거리</th>
        <th>평균속도</th>
        <th>칼로리</th>
        <th>산책 사진</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#walk}}
        <tr>
            <td>{{walkId}}</td>
            <td>{{year}}</td>
            <td>{{month}}</td>
            <td>{{day}}</td>
            <td>{{time}}</td>
            <td>{{distance}}</td>
            <td>{{speed}}</td>
            <td>{{calories}}</td>
            <td>{{walkImg}}</td>
        </tr>
    {{/walk}}
    </tbody>
</table>

</div>

{{>layout/footer}}